{
  "fontSize": 12,
  "fontName": "Consolas",
  "fontWeight": "normal",
  "fontItalic": false,
  "fontUnderline": false,
  "fontStrikeout": false,
  "fontStretch": "normal",
  "textColor": "#000000",
  "backgroundColor": "#ffffff",
  "selectionColor": "#ffffff",
  "selectionBackgroundColor": "#007acc",
  "lineHeight": 1.2,
  "lineSpacing": 0.0,
  "wordWrap": "off",
  "wordWrapWidth": 100,
  "wordWrapMargin": 0.0,
  "tabWidth": 4,
  "cursorColor": "#000000",
  "cursorWidth": 2,
  "cursorBlinking": "solid",
  "cursorBlinkingPeriod": 510,
  "autocomplete": true,
  "autoSuggestions": true,
  "parameterHints": true,
  "calltips": true,
  "hovers": "on",
  "showWhitespace": false,
  "showIndentationGuides": true,
  "showLineNumbers": true,
  "showMargin": true,
  "allowFolding": true,
  "foldingStrategy": "auto",
  "highlightCurrentLine": true,
  "highlightMatchingBrackets": true,
  "undoLimit": 0,
  "useTabStops": false,
  "rulers": [],
  "goToLineOnClick": true
}
